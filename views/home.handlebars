<script src="https://maps.googleapis.com/maps/api/js?v=3&signed_in=false&key=AIzaSyD2R0wdWoFnfcjg2Hi3yvzfH9_BAuJZYQY"></script>
<script>
	// In this example, we center the map, and add a marker, using a LatLng object
	// literal instead of a google.maps.LatLng object. LatLng object literals are
	// a convenient way to add a LatLng coordinate and, in most cases, can be used
	// in place of a google.maps.LatLng object.

	var map;
	function initialize() {
		var mapOptions = {
		zoom: 11,
		center: {lat: 1.36, lng: 103.82},
		disableDefaultUI: true
		};
		map = new google.maps.Map(document.querySelector('.map-canvas'), mapOptions);
	}
	google.maps.event.addDomListener(window, 'load', initialize);

</script>

<div class="map-canvas full-page" style="position: relative" tabindex="-1"></div>
